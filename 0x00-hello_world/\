#include <stdio.h>


int main(void)
{
	int a;
	float b;
	char c;
	long int d;
	long long int e;

printf("size of int : %lu), (unsigned long)size of (a)");
printf("size of float : %lu), (unsigned long)size of (b)");
printf("size of char : %c), (unsigned long)size of (c)");
printf("size of long int : %lu), (unsigned long)size of (d)");
printf("size of long long long int : %llu), (unsigned long)size of (e)");

return (0);
}
